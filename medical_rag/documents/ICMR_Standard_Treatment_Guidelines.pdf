var FOCUSABLE = 'a, button, input, select, textarea, [tabindex]';

function fixOwlAriaHidden() {
  var carousels = document.querySelectorAll('.owl-carousel');

  for (var i = 0; i < carousels.length; i++) {
    var items = carousels[i].querySelectorAll('.owl-item');

    for (var j = 0; j < items.length; j++) {
      var item = items[j];
      var isActive = item.className.indexOf('active') !== -1;

      // Correct aria-hidden
      item.setAttribute('aria-hidden', isActive ? 'false' : 'true');

      var focusables = item.querySelectorAll(FOCUSABLE);

      for (var k = 0; k < focusables.length; k++) {
        var el = focusables[k];

        if (isActive) {
          if (el.getAttribute('tabindex') === '-1') {
            el.removeAttribute('tabindex');
          }
        } else {
          el.setAttribute('tabindex', '-1');
        }
      }
    }
  }
}

/* Run after page load */
window.onload = function () {
  fixOwlAriaHidden();
};

/* Re-run after Owl carousel updates */
document.addEventListener('initialized.owl.carousel', fixOwlAriaHidden);
document.addEventListener('changed.owl.carousel', fixOwlAriaHidden);
document.addEventListener('translated.owl.carousel', fixOwlAriaHidden);

function fixStatsOwlAria() {
  var items = document.getElementsByClassName('owl-item');

  for (var i = 0; i < items.length; i++) {
    var item = items[i];
    var isActive = item.className.indexOf('active') !== -1;

    // Fix aria-hidden on slides
    item.setAttribute('aria-hidden', isActive ? 'false' : 'true');

    // Fix stats container
    var stats = item.getElementsByClassName('statistics__container-conts');
    if (stats.length > 0) {
      stats[0].setAttribute('aria-hidden', isActive ? 'false' : 'true');
      stats[0].removeAttribute('tabindex');
    }
  }
}

/* Run with delay (CMS-safe, no event listeners) */
